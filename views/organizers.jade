extends layout

block content
  h1 About the Organizers

  .row(itemscope="itemscope", itemtype="http://schema.org/Person")
    .span2
      img.img-rounded(src="/images/cory-profile-140.png", height="140", width="140")
    .span10
      h3(itemprop="name") Cory Kaufman-Schofield
      p
        = ' Cory is a developer at '
        span(itemprop="affiliation", itemscope="itemscope", itemtype="http://schema.org/LocalBusiness")
          a(href="http://www.alfajango.com", itemprop="url")
            span(itemprop="name") AlfaJango
          = ' in the '
          a(href="http://www.techbrewery.org", itemprop="location") TechBrewery
        | . He's also got a few side projects that take up way less of his time than he would like.
      p Website:&nbsp;
        a(href="http://corykaufman.com") corykaufman.com
      p Twitter:&nbsp;
        a(href="http://twitter.com/allspiritseve") @allspiritseve
      p Github:&nbsp;
        a(href="http://github.com/allspiritseve") allspiritseve
  br(clear="both")
  .row
    .span2
      img.img-rounded.hi-res(src="/images/cait-profile-140.png", height="140", width="140")
    .span10
      h3 Cait Holman
      p= 'Cait is a PhD student at the University of Michigan School of Information, and the project manager at '
        a(href="http://www.alfajango.com/") AlfaJango
        .
      p Twitter:&nbsp;
        a(href="http://twitter.com/chcholman")@chcholman
      p Github:&nbsp;
        a(href="http://github.com/chcholman") chcholman

  br(clear="both")
  .row
    .span2
      img.img-rounded.hi-res(src="/images/kevin-profile-140.png", height="140", width="140")
    .span10
      h3 Kevin Champion
      p Kevin is a freelance web developer who specializes in Drupal. He thinks favicons are important.
      p Twitter:&nbsp;
        a(href="http://twitter.com/kevinchampion")@kevinchampion
      p Github:&nbsp;
        a(href="http://github.com/kevinchampion") kevinchampion
  br(clear="both")

